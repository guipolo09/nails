{
  "expo": {
    "name": "Nails Calendar",
    "slug": "nails-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E91E63"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.guilhermepolo09.nailsapp",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Este app precisa acessar seu calend√°rio para criar lembretes de agendamentos.",
        "NSRemindersUsageDescription": "Este app precisa acessar lembretes para notificar sobre seus agendamentos."
      }
    },
    "android": {
      "package": "com.guilhermepolo09.nailsapp",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E91E63"
      },
      "permissions": [
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "d3a89413-7d0a-4a6b-93b8-be15fd033211"
      }
    },
    "plugins": [
      "expo-sqlite"
    ]
  }
}
