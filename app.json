{
  "expo": {
    "name": "Nails Calendar",
    "slug": "nails-calendar-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E91E63"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nails.app",
      "infoPlist": {
        "NSCalendarsUsageDescription": "Este app precisa de acesso ao calendário para salvar seus agendamentos no Google Agenda.",
        "NSCalendarsFullAccessUsageDescription": "Este app precisa de acesso ao calendário para salvar seus agendamentos no Google Agenda."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#E91E63"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nails.app",
      "permissions": [
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-calendar",
        {
          "calendarPermission": "Este app precisa de acesso ao calendário para salvar seus agendamentos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "d3a89413-7d0a-4a6b-93b8-be15fd033211"
      }
    }
  }
}